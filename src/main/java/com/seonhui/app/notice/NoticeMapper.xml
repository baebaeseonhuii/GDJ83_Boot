<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.seonhui.app.notice.NoticeMapper">
 	<select id="getList" resultType="NoticeVO">
 		SELECT * FROM NOTICE ORDER BY BOARD_NUM DESC
 	</select>
 	
 	<insert id="addNotice" parameterType="NoticeVO">
 		INSERT INTO NOTICE VALUES(#{board_num}, #{board_writer}, #{board_title}, #{board_contents}, NOW())
 	</insert>
 
 </mapper>